// eslint-disable-next-line no-undef
{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended"
    ],
    "env": {
        "node": true,
        "es2021": true
    },
    "rules": {
        "no-console": "error",
        "prettier/prettier": [
          "error",
          {
            "endOfLine": "auto"
          }
        ]
    }
}
